{
  "name": "poller",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test,scripts}/**/*.ts\" --fix",
    "test": "NODE_ENV=test jest --runInBand",
    "test:watch": "NODE_ENV=test jest --runInBand --watch",
    "test:cov": "NODE_ENV=test jest --runInBand --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --runInBand --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/bull": "^0.6.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.0.11",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.1",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.0.11",
    "axios": "^0.27.2",
    "bull": "^4.8.5",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.13.2",
    "joi": "^17.6.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "pg": "^8.8.0",
    "puppeteer": "^17.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.9",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bull": "^3.15.9",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.2",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@persistence/(.*)": "<rootDir>/src/persistence/$1",
      "@background-process/(.*)": "<rootDir>/src/background-process/$1",
      "@api/(.*)": "<rootDir>/src/api/$1",
      "@interfaces/(.*)": "<rootDir>/src/interfaces/$1",
      "@notifiers/(.*)": "<rootDir>/src/notifiers/$1",
      "@rules/(.*)": "<rootDir>/src/rules/$1",
      "@test/(.*)": "<rootDir>/test/$1",
      "@scraping/(.*)": "<rootDir>/src/scraping/$1",
      "@util/(.*)": "<rootDir>/src/util/$1",
      "@transformations/(.*)": "<rootDir>/src/transformations/$1"
    }
  },
  "_moduleAliases": {
    "@persistence": "./src/persistence",
    "@background-process": "./src/background-process",
    "@api": "./src/api",
    "@interfaces": "./src/interfaces",
    "@notifiers": "./src/notifiers",
    "@rules": "./src/rules",
    "@test": "./test",
    "@scraping": "./src/scraping",
    "@util": "./src/util",
    "@transformations": "./src/transformations"
  }
}
